<Window x:Class="FocuSpider.PresetDialogbox"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FocuSpider"
        mc:Ignorable="d"
        WindowStartupLocation="CenterOwner"
        Title="Preset settings" Height="159" Width="221" ResizeMode="NoResize">
    <Grid Margin="0,0,0,0">
        <Button x:Name="okBtn" Content="OK" HorizontalAlignment="Left" Margin="10,84,0,0" VerticalAlignment="Top" Height="28" Width="86" Click="okBtn_Click"/>
        <Button x:Name="cancelBtn" Content="Cancel" HorizontalAlignment="Left" Margin="112,84,0,0" VerticalAlignment="Top" Width="86" Height="28" Click="cancelBtn_Click"/>
        <TextBox x:Name="txtName" HorizontalAlignment="Left" Margin="78,10,0,0" TextWrapping="Wrap" Text="PresetName" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="txtSteps" HorizontalAlignment="Left" TextWrapping="Wrap" Text="800" VerticalAlignment="Top" Width="120" Height="25" Margin="78,39,0,0"/>
        <Label x:Name="lblName" Content="Name:" HorizontalAlignment="Left" Margin="25,6,0,0" VerticalAlignment="Top"/>
        <Label x:Name="lblSteps" Content="Steps:" HorizontalAlignment="Left" Margin="28,37,0,0" VerticalAlignment="Top"/>

    </Grid>
</Window>
